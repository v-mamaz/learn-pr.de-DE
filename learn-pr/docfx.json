{
  "build": {
    "content": [{
        "files": [
          "achievements.yml"
        ],
        "dest": "achievements/main/"
      },
      {
        "src": "azure",
        "files": [
          "**.yml"
        ],
        "dest": "modules"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [{
      "files": [
        "**/*.png",
        "**/*.svg",
        "**/*.jpg"
      ],
      "exclude": [
        "**/obj/**",
        "**/includes/**",
        "_themes/**",
        "_themes.pdf/**"
      ]
    }],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/learn/breadcrumb/toc.json",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/learn",
      "clicktale": true
    },
    "template": [],
    "dest": "learn-pr",
    "markdownEngineName": "markdig"
  }
}
